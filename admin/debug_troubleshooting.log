[2025-11-20T13:45:08+01:00] CURRENT_ROW: id=65 ; row={"id":65,"brand":"SAMSUNG","device_type":"REFRIGERATOR","parent_id":62,"title":"Water Leaking Inside or Outside","description":"Water pooling inside refrigerator or on floor around appliance","fix_steps":"Check if refrigerator is level - adjust feet if needed\r\nInspect drain hole at back of fridge compartment for clogs\r\nClean drain hole with warm water and pipe cleaner\r\nVerify drain pan underneath is positioned correctly\r\nCheck water line connections for leaks\r\nEnsure door closes completely and seals properly\r\nRemove ice buildup in freezer that may be blocking drain\r\nCheck if humidity is causing excessive condensation\r\nVerify defrost drain tube is not frozen or blocked","steps_json":null,"preventive_tip":"Keep drain hole clear. Level refrigerator properly during installation. Don't leave doors open.","decision_logic":null,"image_path":null,"display_order":3,"image":null,"created_at":"2025-11-17 20:34:51","is_active":1,"updated_at":"2025-11-17 20:34:51"}
