[2025-11-20T13:49:45+01:00] CURRENT_ROW: id=51 ; row={"id":51,"brand":"SHARP","device_type":"AIR_CONDITIONER","parent_id":48,"title":"Water Leaking from Indoor Unit","description":"Water dripping from front or sides of indoor air conditioning unit","fix_steps":"Check drain hose is not kinked or blocked\r\nEnsure drain hose slopes downward - no upward loops\r\nClean drain pan and check for cracks\r\nClear drain line blockage with compressed air or wet\/dry vacuum\r\nVerify AC is installed level - tilt can cause improper drainage\r\nCheck if drain pump is working (if equipped)\r\nInspect for frozen evaporator coil - indicates airflow problem\r\nClean air filters to improve airflow\r\nIn humid conditions, increase fan speed setting\r\nSchedule professional drain line cleaning if problem persists","steps_json":null,"preventive_tip":"Clean filters regularly. Ensure proper installation slope. Annual drain line maintenance recommended.","decision_logic":null,"image_path":null,"display_order":3,"image":null,"created_at":"2025-11-17 20:33:37","is_active":1,"updated_at":"2025-11-17 20:33:37"}
[2025-11-20T13:49:45+01:00] UPDATE: SQL=UPDATE troubleshooting_guide SET `parent_id` = ?, `brand` = ?, `device_type` = ?, `title` = ?, `description` = ?, `fix_steps` = ?, `preventive_tip` = ?, `display_order` = ?, `is_active` = ?, `image_path` = ? WHERE id = ? ; values=[48,"SHARP","AIR_CONDITIONER","Water Leaking from Indoor Unit","Water dripping from front or sides of indoor air conditioning unit","Check drain hose is not kinked or blocked\r\nEnsure drain hose slopes downward - no upward loops\r\nClean drain pan and check for cracks\r\nClear drain line blockage with compressed air or wet\/dry vacuum\r\nVerify AC is installed level - tilt can cause improper drainage\r\nCheck if drain pump is working (if equipped)\r\nInspect for frozen evaporator coil - indicates airflow problem\r\nClean air filters to improve airflow\r\nIn humid conditions, increase fan speed setting\r\nSchedule professional drain line cleaning if problem persists","Clean filters regularly. Ensure proper installation slope. Annual drain line maintenance recommended.",3,1,"uploads\/troubleshooting\/guide_1763642985_5dd61e5f2c44.jpg",51] ; rowCount=1
